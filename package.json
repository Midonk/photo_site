{
  "name": "tooling",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browserslist": [
    "last 2 versions",
    "not ie <= 11"
  ],
  "scripts": {
    "build:11ty": "npx eleventy --quiet",
    "build:sass": "node-sass ./src/assets/scss/main.scss ./dist/assets/css/main.css --output-style compressed",
    "build:js": "webpack --config webppackConfig.js",
    "build:img": "imagemin ./src/assets/img/**/* --out-dir ./src/assets/img",
    "del:dist": "del-cli ./dist",
    "build": "npm-run-all del:dist build:img -p build:11ty build:sass build:js",
    "watch:sass": "onchange ./src/assets/scss/**/* -- npm run build:sass",
    "watch:11ty": "onchange ./src/**/* -e ./src/assets/scss/**/* -- npm run build:11ty",
    "serve": "browser-sync start --server dist --files \"dist/**/*\"",
    "watch": "npm-run-all build -p serve watch:**"
  },
  "keywords": [],
  "author": "TF",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "autoprefixer": "^9.7.2",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "del-cli": "^3.0.0",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "imagemin-cli": "^5.0.0",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "sharp": "^0.23.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
